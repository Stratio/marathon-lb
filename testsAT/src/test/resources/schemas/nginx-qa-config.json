{
  "labels": {
    "HAPROXY_0_BACKEND_WEIGHT": "0",
    "DCOS_SERVICE_SCHEME": "http",
    "DCOS_SERVICE_NAME": "nginx-qa",
    "HAPROXY_0_GROUP": "external",
    "DCOS_SERVICE_PORT_INDEX": "0",
    "DCOS_PACKAGE_NAME": "nginx-qa",
    "HAPROXY_0_PATH": null,
    "HAPROXY_0_VHOST": "nginx-qa.labs.stratio.com"
  },
  "id": "/nginx-qa",
  "acceptedResourceRoles": [
    "*"
  ],
  "backoffFactor": 1.15,
  "backoffSeconds": 1,
  "container": {
    "portMappings": [
      {
        "containerPort": 80,
        "labels": {
          "VIP_0": "/nginx-qa-minuteman:1234"
        },
        "servicePort": 0
      }
    ],
    "type": "DOCKER",
    "volumes": [],
    "docker": {
      "image": "qa.stratio.com/nginx:1.10.3-alpine",
      "forcePullImage": false,
      "privileged": false,
      "parameters": []
    }
  },
  "cpus": 1,
  "disk": 0,
  "healthChecks": [
    {
      "gracePeriodSeconds": 100,
      "ignoreHttp1xx": false,
      "intervalSeconds": 20,
      "maxConsecutiveFailures": 3,
      "portIndex": 0,
      "timeoutSeconds": 20,
      "delaySeconds": 15,
      "protocol": "HTTP",
      "path": "/",
      "ipProtocol": "IPv4"
    }
  ],
  "instances": 1,
  "maxLaunchDelaySeconds": 3600,
  "mem": 256,
  "gpus": 0,
  "networks": [
    {
      "name": "stratio",
      "mode": "container"
    }
  ],
  "requirePorts": false,
  "upgradeStrategy": {
    "maximumOverCapacity": 1,
    "minimumHealthCapacity": 1
  },
  "killSelection": "YOUNGEST_FIRST",
  "unreachableStrategy": {
    "inactiveAfterSeconds": 0,
    "expungeAfterSeconds": 0
  },
  "fetch": [],
  "constraints": []
}